{
  "expo": {
    "name": "Moja Biblioteczka",
    "slug": "moja-biblioteczka",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.mojabiblioteczka"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Aplikacja potrzebuje dostępu do twoich zdjęć, aby dodać okładkę książki.",
          "cameraPermission": "Aplikacja potrzebuje dostępu do aparatu, aby zrobić zdjęcie okładki książki."
        }
      ]
    ]
  }
}