{
  "expo": {
    "name": "Moja Biblioteczka",
    "slug": "moja-biblioteczka",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },

      "package": "com.yourcompany.mojabiblioteczka",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Aplikacja potrzebuje dostępu do twoich zdjęć, aby dodać okładkę książki.",
          "cameraPermission": "Aplikacja potrzebuje dostępu do aparatu, aby zrobić zdjęcie okładki książki."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f9594af7-5c3f-486f-aacc-a0ba781423df"
      }
    }
  }

